{
  "printWidth": 130,
  "singleQuote": true,
  "trailingComma": "es5",
  "proseWrap": "always",
  "tabWidth": 2,
  "semi": true,
  "arrowParens": "avoid",
  "overrides": [{ "files": ".prettierrc", "options": { "parser": "json" } }],
  "plugins": ["prettier-plugin-organize-imports", "prettier-plugin-packagejson", "prettier-plugin-tailwindcss"]
}
